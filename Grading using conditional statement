#include<stdio.h>
#include<conio.h>
int main()
{
    int i;
    printf("Enter marks:");
    scanf("%d",&i);
    if(i>=85)
    {
        printf("Grade A");
    }
    else if(i>=70&&i<85)
    {
        printf("Grade B");
    }
    else if(i>=55&&i<70)
    {
        printf("Grade C");
    }
    else if(i>=40&&i<55)
    {
        printf("Grade D");
    }
    else if(i<40)
    {
        printf("Grade F");
    }
    else
    {
        printf("Enter valid marks");
    }
    getch();
    return 0;
}

